/* ************************************************************** */
/* All Screens under 960px */
/* ************************************************************** */
@media screen and (max-width: 959px) {
	.gbtr_header_wrapper .header_phones{
		display: none;
	}
	
	.c_block_your_local{
		background: none;
		margin-bottom: 20px;
	}
	.c_block_your_local .content_wrapper{
		padding: 0;
	}
	.blue_content{
		width: 100% !important;
	}
	
	.c_our_vet_s{
		margin-top: 20px;
	}
	
	#quoterotator{
		margin-left: 200px;
		text-shadow: 1px 1px #555;
	}
	
	#our_brands h1:after{
		border-bottom: none;
	}
	
	#newsletter_subscribe{
		height: auto;
	}
	.my_pages .row-fluid .span3{
		width: 100% !important;
	}
	
	.my_pages .row-fluid .span4{
		width: 100% !important;
	}
	
	#my_footer, #newsletter_subscribe{
		padding: 0 10px;
	}
}

/* ************************************************************** */
/* Big Screens > 960px */
/* ************************************************************** */
@media screen and (min-width: 960px) {

}

/* ************************************************************** */
/* Tablet 720px - 960px */
/* ************************************************************** */
@media only screen and (min-width: 720px) and (max-width: 959px) {
	.above_footer .grid_4{
		max-height: auto;
	    overflow: hidden;
	}
}

/* ************************************************************** */
/* Mobile 0px - 720px */
/* ************************************************************** */
@media only screen and (max-width: 719px) {
	.special_block_content{
		margin: 0 auto;
		margin-bottom: 20px;
	}
	.testimonials1{
		clear: both;
	}
	.testimonials{
		background: none;
		position: inherit;
		margin-top: 20px;
		clear: both;
		height: auto !important;
	}
	#quoterotator{
		margin: 0;
		padding: 20px;
		width: 80%;
	}
	
	.above_footer{
		padding-top: 0;
	}
	
	#footer_news .grid_2{
		width: 100%;
	}
	
	.gray_li_wide{
		width: 100% !important;
	}
	
	.widget_wp_news_events_widget h1{
		bottom: 23px;
	    font-size: 17px;
	    left: 20px;
	    position: absolute !important;
	    text-transform: uppercase;
	    width: 11%;
	}
	
}

@media only screen and (max-width: 767px) {
.home-banner-custom { display: none; }
}

